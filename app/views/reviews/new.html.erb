<h1>New review</h1>


<%= form_for [@restaurant, @review] do |f| %>
  <div class="field">
    <%= f.label :review_content %><br />
    <%= f.text_field :review_content %>
  </div>
  <div class="field">
    <%= f.label :reviewer_name %><br />
    <%= f.text_field :reviewer_name %>
  </div>
   <%= f.submit %>
<% end %>

<%= link_to 'Back', restaurant_reviews_path(@review.restaurant) %>
